<h1>Contact Us</h1>
<%= simple_form_for(@contact) do |f| %>
  
  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :email %>
  
    <%= f.input :address %>
    <%= f.input :comments %>
  
    
    
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

